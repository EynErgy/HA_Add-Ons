{
  "name": "heimdall",
  "version": "0.1.0",
  "slug": "heimdall",
  "init": false,
  "description": "Application Dashboard",
  "url": "https://github.com/EynErgy/HA_Add-Ons/tree/master/heimdall#readme",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "ports": {
    "80/tcp": 82,
    "443/tcp": 8445
  },
  "ports_description": {
    "80/tcp": "web interface",
    "443/tcp": "web interface (SSL)"
  },
  "map": [
    "addon_config:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:82]",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  },
  "environment": {},
  "image": "EynErgy/ha-addons-heimdall-{arch}"
}
